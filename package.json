{
  "name": "express.js scafold",
  "version": "1.0.0",
  "main": "server.ts",
  "author": "Beau Iyere",
  "license": "ISC",
  "description": "a hotel booking application with node.js",
  "repository": {
    "type": "git",
    "url": ""
  },
  "type": "module",
  "scripts": {
    "build": "rimraf .dist && npx tsc && tsc-alias",
    "start:dev": "cross-env NODE_ENV=development tsx watch src/server.ts",
    "start:prod": "cross-env NODE_ENV=production && pnpm run build && node ./dist/src/server.js",
    "lint": "NODE_NO_WARNINGS=1 eslint .",
    "lint-fix": "NODE_NO_WARNINGS=1 eslint . --fix",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "clear": "jest --clearCache",
    "start:test": "cross-env NODE_ENV=test NODE_CONFIG_DIR=./config vitest run",
    "start:test:openHandles": "cross-env NODE_ENV=test NODE_CONFIG_DIR=./config vitest run --reporter=verbose",
    "test:watch": "cross-env NODE_ENV=test NODE_CONFIG_DIR=./config vitest",
    "test:coverage": "cross-env NODE_ENV=test NODE_CONFIG_DIR=./config vitest run --coverage",
    "test:debug": "cross-env NODE_ENV=test NODE_CONFIG_DIR=./config node --inspect-brk ./node_modules/.bin/vitest run",
    "test:debug:watch": "cross-env NODE_ENV=test NODE_CONFIG_DIR=./config node --inspect-brk ./node_modules/.bin/vitest",
    "test:debug:coverage": "cross-env NODE_ENV=test NODE_CONFIG_DIR=./config node --inspect-brk ./node_modules/.bin/vitest run --coverage",
    "test:cache": "vitest --clearCache",
    "test:ci": "cross-env NODE_ENV=test NODE_CONFIG_DIR=./config vitest run --coverage --reporter=verbose",
    "prepare": "husky && husky install",
    "precommit": "eslint ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "bullmq": "^5.69.3",
    "compression": "^1.8.1",
    "connect-redis": "^9.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "cross-env": "^10.1.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.19",
    "decimal.js": "^10.6.0",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.19.0",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.9.3",
    "ip": "^2.0.1",
    "jsonwebtoken": "^9.0.3",
    "minio": "^8.0.6",
    "mongodb": "^7.1.0",
    "mongoose": "^9.2.1",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^8.0.1",
    "pino": "^10.3.1",
    "pino-pretty": "^13.1.3",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.1",
    "tsyringe": "^4.10.0",
    "uuid": "^13.0.0",
    "validator": "^13.15.26",
    "winston": "^3.19.0",
    "yamljs": "^0.3.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^10.0.1",
    "@types/compression": "^1.8.1",
    "@types/config": "^3.3.5",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/cryptojs": "^3.1.33",
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.18.2",
    "@types/ioredis-mock": "^8.2.6",
    "@types/ip": "^1.1.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.23",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.3.0",
    "@types/nodemailer": "^7.0.10",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/validator": "^13.15.10",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "eslint": "^9.39.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-sonarjs": "^4.0.0",
    "eslint-plugin-unicorn": "^63.0.0",
    "flatted": "^3.3.3",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "ioredis-mock": "^8.13.1",
    "lint-staged": "^16.2.7",
    "lodash": "^4.17.23",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.3",
    "supertest": "^7.2.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18"
  }
}
